{% include header %}
{% include breadcrumb %}
{% include content %}
<div
  class="gallery pure-g">
  {% for img in images %}
    <div
      class="pure-u-sm-1 pure-u-md-1-2 pure-u-lg-1-3 pure-u-xl-1-4">
      <a
        href="/{{ img.type }}/{{ img.year }}/{{ img.index }}">
        <div
          class="gallery__thumbnail">
          {% galleryThumbnailImage img.type img.year img.file %}
        </div>
      </a>
    </div>
  {% endfor %}
</div>